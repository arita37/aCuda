file = '005_01_bat'
size = 1

set terminal png font 'segoe ui,10'
set output 'D:\MGR\aCuda\doc\figures\06\'.file.'.png'
set ylabel 'czas (ms)'
set xlabel 'liczba transakcji'
set pointsize size
set key left
set title 'Prędkość działania na baterii (minsup = 0.05, minconf = 0.1)'


plot [0:35000] [0:] 'data\'.file.'.txt' using 1:2 title columnhead with lp lw size, \
 'data\'.file.'.txt' using 1:3 title columnhead with lp lw size, \
 'data\'.file.'.txt' using 1:4 title columnhead with lp lw size, \
 'data\'.file.'.txt' using 1:5 title columnhead with lp lw size

 unset output